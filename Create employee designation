emp={}

n=int(input('enter no.of employess:'))

for i in range(1,n+1):

    l=[]

    empname=input('enter empname:')

    l.append(empname)

    empdesig=input('enter empdesignation:')

    l.append(empdesig)

    empsal=input('enter employe salary:')

    l.append(empsal)

    empphno=input('enter employe phonenumber:')

    l.append(empphno)

    emp[i]=l

    print(emp)
